{
  "name": "fechatter_frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Modern team chat application with proxy-based backend integration",
  "scripts": {
    "dev": "node scripts/copy-configs.js && echo '\n🚀 启动 Fechatter 开发服务器...\n💡 服务器启动后，请手动打开浏览器访问显示的 URL\n📱 主要访问地址: http://localhost:5173\n━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\n' && npx vite",
    "dev:auto-open": "node scripts/copy-configs.js && npx vite --open",
    "dev:with-bot": "chmod +x scripts/start-with-bot-mock.sh && scripts/start-with-bot-mock.sh",
    "dev:bot-only": "cd server && npm install && node bot-mock.js",
    "build": "node scripts/copy-configs.js && npx vue-tsc && npx vite build",
    "build:dev": "node scripts/copy-configs.js && npx vue-tsc && npx vite build --mode development",
    "preview": "npx vite preview",
    "tauri": "tauri",
    "copy-configs": "node scripts/copy-configs.js",
    "cleanup-logs": "node scripts/cleanup-logs.js",
    "cleanup-logs:dry": "node scripts/cleanup-logs.js --dry-run",
    "build:clean": "yarn cleanup-logs && yarn build",
    "generate-proto": "node scripts/generate-proto.js",
    "prebuild": "yarn generate-proto",
    "lint": "echo 'Linting not configured yet'",
    "type-check": "npx vue-tsc --noEmit",
    "clean": "rm -rf node_modules yarn.lock package-lock.json",
    "fresh-install": "yarn clean && yarn install",
    "dev:clean": "yarn copy-configs && npx vite --force"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@types/markdown-it": "^14.1.2",
    "@vueuse/core": "^11.3.0",
    "axios": "^1.7.9",
    "browser-image-compression": "^2.0.2",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "emoji-js": "^3.8.1",
    "emoji-picker-react": "^4.12.2",
    "hast-util-from-parse5": "^8.0.3",
    "highlight.js": "^11.11.1",
    "lucide-vue-next": "^0.513.0",
    "markdown-it": "^14.1.0",
    "markdown-it-emoji": "^3.0.0",
    "marked": "^15.0.12",
    "mermaid": "^11.6.0",
    "nprogress": "^0.2.0",
    "parse5": "^7.3.0",
    "photoswipe": "^5.4.4",
    "pinia": "^2.3.0",
    "prismjs": "^1.30.0",
    "protobufjs": "^7.4.0",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "shiki": "^0.14.7",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "uuid": "^11.1.0",
    "viewerjs": "^1.11.7",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/nprogress": "^0.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.21",
    "glob": "^11.0.3",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.2.2",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.0.21"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  }
}